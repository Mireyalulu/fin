<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="fin.Administrador">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="ADMINISTRADOR" FontSize="Large" HorizontalOptions="Center"/>
            <Image Source="admin.jpg" HorizontalOptions="Center" VerticalOptions="Center" Aspect="AspectFill" />
            <Label x:Name="id_user"></Label>
            <Label x:Name="nombre"></Label>
            <Button x:Name="Asignar_tareas" Text="Asignar tareas" Clicked="Asignar_tareas_Clicked"></Button>
            <Button x:Name="Registrarse" Text="Registrarse" Clicked="Registrarse_Clicked" BackgroundColor="Plum"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>